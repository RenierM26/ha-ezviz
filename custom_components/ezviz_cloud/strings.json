{
  "title": "EZVIZ Cloud",
  "config": {
    "step": {
      "user": {
        "title": "Connect EZVIZ Cloud",
        "description": "Sign in to your EZVIZ Cloud account and choose the API region. If you select Custom, enter the url (without https://).",
        "data": {
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]",
          "region": "Region",
          "url": "[%key:common::config_flow::data::url%]",
          "timeout": "[%key:common::config_flow::data::timeout%]"
        }
      },
      "user_mfa_confirm": {
        "title": "Two-factor verification required",
        "description": "Enter the SMS verification code sent by EZVIZ.",
        "data": {
          "sms_code": "[%key:common::config_flow::data::code%]"
        }
      },
      "reauth_confirm": {
        "title": "[%key:common::config_flow::title::reauth%]",
        "description": "Your session expired or became invalid. Enter your password to refresh tokens. You may be asked for an SMS code next.",
        "data": {
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]"
        }
      },
      "reauth_mfa": {
        "title": "Re-authenticate: SMS code",
        "description": "Enter the SMS verification code to finish re-authentication.",
        "data": {
          "sms_code": "[%key:common::config_flow::data::code%]"
        }
      }
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "invalid_auth": "[%key:common::config_flow::error::invalid_auth%]",
      "invalid_host": "[%key:common::config_flow::error::invalid_host%]",
      "invalid_url": "Custom API host is invalid. Enter a hostname without https:// or trailing slash.",
      "mfa_required": "Multi-factor authentication required, please enter the code",
      "device_exception": "Device exception, couldn't fetch key",
      "verification_required": "Verification code required or invalid. Please try again.",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    },
    "abort": {
      "already_configured_account": "[%key:common::config_flow::abort::already_configured_account%]",
      "reauth_successful": "[%key:common::config_flow::abort::reauth_successful%]",
      "ezviz_cloud_account_missing": "Ezviz cloud account missing. Please reconfigure Ezviz cloud account",
      "no_cameras": "No cameras were found for this account.",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    }
  },
  "options": {
    "step": {
      "init": {
        "title": "EZVIZ Options",
        "description": "Configure cloud or camera options.",
        "menu_options": {
          "cloud": "Cloud Settings",
          "camera_select": "Camera Settings"
        }
      },
      "cloud": {
        "title": "Cloud Settings",
        "description": "Set default options for the EZVIZ cloud account.",
        "data": {
          "timeout": "[%key:common::config_flow::data::timeout%]"
        }
      },
      "camera": {
        "title": "Select Camera",
        "description": "Choose a camera to configure.",
        "data": {
          "serial": "Camera"
        }
      },
      "camera_select": {
        "title": "Select a camera",
        "description": "Choose a camera to edit."
      },
      "camera_edit": {
        "title": "Camera settings",
        "description": "Editing camera {serial} at {ip_address}. Set RTSP credentials for this camera. If you don't know them, choose \"fetch_my_key\" and provide one-time codes on the next step if prompted.",
        "data": {
          "username": "[%key:common::config_flow::data::username%]",
          "password": "Verification code (older cams) or leave “fetch_my_key”",
          "enc_key": "Encryption key (newer cams) or leave “fetch_my_key”",
          "rtsp_uses_verification_code": "Use verification code for RTSP (instead of encryption key)",
          "ephemeral_test_rtsp": "Test RTSP now (wakes device and probes stream)",
          "ffmpeg_arguments": "RTSP path (e.g. /Streaming/Channels/102)"
        }
      },
      "camera_edit_2fa": {
        "title": "One-time codes",
        "description": "Enter one-time codes for {serial} at {ip_address} to fetch verification/encryption keys.",
        "data": {
          "cam_verification_2fa_code": "[%key:common::config_flow::data::code%]",
          "cam_encryption_2fa_code": "[%key:common::config_flow::data::code%]"
        }
      }
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "device_exception": "Device exception, couldn't fetch key",
      "verification_required": "Verification code required or invalid. Please try again.",
      "unknown": "[%key:common::config_flow::error::unknown%]",
      "rtsp_auth_failed": "[%key:common::config_flow::error::rtsp_auth_failed%]"
    }
  },
